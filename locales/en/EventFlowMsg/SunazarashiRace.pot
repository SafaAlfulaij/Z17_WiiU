msgctxt "0008"
msgid "Let's do this."
msgstr ""

msgctxt "0009"
msgid "Hold on."
msgstr ""

msgctxt "0010"
msgid "Sand-seal race?"
msgstr ""

msgctxt "0011"
msgid "Tali?"
msgstr ""

msgctxt "0014"
msgid "Of course."
msgstr ""

msgctxt "0015"
msgid "I'll pass."
msgstr ""

msgctxt "0016"
msgid "OK."
msgstr ""

msgctxt "0017"
msgid "Forget it."
msgstr ""

msgctxt "0018"
msgid "Tell me the rules."
msgstr ""

msgctxt "0019"
msgid "I want to try again."
msgstr ""

msgctxt "0020"
msgid "Not right now."
msgstr ""

msgctxt "0021"
msgid "Who are you?"
msgstr ""

msgctxt "0022"
msgid "Good-bye."
msgstr ""

msgctxt "0023"
msgid "I'm ready to race. "
msgstr ""

