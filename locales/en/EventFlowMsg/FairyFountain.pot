msgctxt "0002"
msgid "Oh, I found it."
msgstr ""

msgctxt "0003"
msgid "I had no luck..."
msgstr ""

msgctxt "0004"
msgid "Recognize anything?"
msgstr ""

msgctxt "0005"
msgid "Not yet..."
msgstr ""

msgctxt "0006"
msgid "I did—thanks!"
msgstr ""

msgctxt "0007"
msgid "Please show me."
msgstr ""

msgctxt "0008"
msgid "No."
msgstr ""

