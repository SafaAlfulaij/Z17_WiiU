msgid "Precisely."
msgstr ""

msgctxt "0005"
msgid "Linny?"
msgstr ""

msgctxt "0006"
msgid "Mipha?"
msgstr ""

msgctxt "0007"
msgid "Who are you?"
msgstr ""

msgctxt "0008"
msgid "Linny?"
msgstr ""

msgctxt "0009"
msgid "Good-bye."
msgstr ""

msgctxt "0010"
msgid "Great!"
msgstr ""

msgctxt "0011"
msgid "OK, I guess..."
msgstr ""

msgctxt "0012"
msgid "You're welcome."
msgstr ""

#. 1: sound
msgctxt "near00"
msgid "<1.so>Helloooo!"
msgstr ""

#. 1: sound
msgctxt "near01"
msgid "<1.so>Linnyyyy!"
msgstr ""

#. 1: sound
#. 2: pause (short)
#. 3: set_colour (blue)
#. 4: reset_colour
#. 5: sound
#. 6: pause (short)
#. 7: pause (longer)
