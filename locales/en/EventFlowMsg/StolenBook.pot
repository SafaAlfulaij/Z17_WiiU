msgid ""
"<1.so>Heh... You sure gave me a fright talking\n"
"to me out of the blue.\n"
"\n"
"<2.te_si><3.se_co>Could it be...he knows?<4.re_co><5.pa>\n"
"<6.se_co>No...<7.re_co><8.pa> <9.se_co>Nobody knows about "
"that.<10.pa><11.re_co><12.te_si>"
msgstr ""

#. 1: sound
msgctxt "Npc_Kakariko008_08"
msgid "<1.so>Heh... You caught me orange-handed."
msgstr ""

#. 1: sound
#. 2: single_choice
msgctxt "Npc_Kakariko008_09"
msgid ""
"<1.so>You mean that garish thing in Impa's\n"
"house?<2.si_ch>"
msgstr ""

#. 1: sound
msgctxt "Npc_Kakariko008_10"
msgid "<1.so>*cough cough*"
msgstr ""

