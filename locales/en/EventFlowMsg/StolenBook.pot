msgstr ""

msgctxt "Npc_Kakariko007_S1_Near009"
msgid "Must be imagining things."
msgstr ""

#. 1: sound
msgctxt "Npc_Kakariko007_S1_Near010"
msgid "<1.so>Show yourself!"
