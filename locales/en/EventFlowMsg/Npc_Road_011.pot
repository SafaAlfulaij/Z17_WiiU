#. 2: sound2
msgctxt "hello"
msgid "<1.so><2.so>Sav'otta!"
msgstr ""

#. 1: sound
#. 2: sound2
msgctxt "hello_00"
msgid "<1.so><2.so>Sav'aaq."
msgstr ""

#. 1: sound
#. 2: sound2
msgctxt "hello_01"
msgid "<1.so><2.so>Sav'saaba!"
msgstr ""

