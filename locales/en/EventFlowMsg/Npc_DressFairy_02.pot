#. 2: text_size
#. 3: text_size
msgctxt "Talk34"
msgid "<1.so><2.te_si>Ahh-haaaaan!<3.te_si>"
msgstr ""

#. 1: sound2
#. 2: text_size
#. 3: text_size
msgctxt "Talk35"
msgid "<1.so><2.te_si>Ooooh-hoooon!!<3.te_si>"
