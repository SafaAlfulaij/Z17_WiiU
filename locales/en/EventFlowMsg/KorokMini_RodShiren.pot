msgctxt "Npc_kokiri012_F_Talk061"
msgid ""
"<1.so>That thing on your back...<2.pa>\n"
"That's an <3.se_co>ice rod<4.re_co>!<5.ra><6.so>Hold up. <7.pa>I was sure"
" you had something\n"
"stronger! Equip it and show me.\n"
"SHOW ME!"
msgstr ""

msgctxt "Npc_kokiri012_R_Near000"
msgid "Mr. Hero..."
msgstr ""

#. 1: sound
#. 2: pause (short)
#. 3: set_colour (red)
#. 4: reset_colour
