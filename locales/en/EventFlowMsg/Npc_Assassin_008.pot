msgctxt "Henshin01"
msgid ""
"<1.te_si><2.so><3.ra>You can offer your apologies to\n"
"Master Kohga in person!<4.au_ad><5.te_si>"
msgstr ""

#. 1: sound2
msgctxt "Near00"
msgid "<1.so>Ehehe..."
msgstr ""

#. 1: sound
#. 2: pause (longer)
#. 3: pause (longer)
#. 4: pause (longer)
