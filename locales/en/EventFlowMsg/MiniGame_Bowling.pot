msgctxt "0000"
msgid "I'll try it!"
msgstr ""

msgctxt "0001"
msgid "How does it work?"
msgstr ""

msgctxt "0002"
msgid "Not interested."
msgstr ""

msgctxt "0003"
msgid "Explain again?"
msgstr ""

msgctxt "0004"
msgid "I've lost interest."
msgstr ""

msgctxt "0005"
msgid "OK!"
msgstr ""

msgctxt "0006"
msgid "I'll pass."
msgstr ""

msgctxt "0007"
msgid "I have to split."
msgstr ""

msgctxt "0008"
msgid "It's nothing."
msgstr ""

msgctxt "0009"
msgid "I quit!"
msgstr ""

msgctxt "0010"
msgid "I'll keep at it."
msgstr ""

msgctxt "0011"
msgid "Oops. My mistake."
msgstr ""

msgctxt "0012"
msgid "I'll do it!"
msgstr ""

#. 1: sound
#. 2: pause (long)
#. 3: pause (long)
#. 4: pause (short)
