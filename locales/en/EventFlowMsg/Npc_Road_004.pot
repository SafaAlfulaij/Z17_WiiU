msgid "Yes."
msgstr ""

msgctxt "5501"
msgid "No."
msgstr ""

msgctxt "5502"
msgid "Buy."
msgstr ""

msgctxt "5503"
msgid "Never mind."
msgstr ""

msgctxt "5507"
msgid "I'll take 'em all!"
msgstr ""

