#. 6: reset_colour
msgctxt "Talk16"
msgid "<1.se_co><2.so><3.re_co>All I need is <4.se_co><5.va> rupees<6.re_co>..."
msgstr ""

#. 1: sound2
#. 2: set_colour (red)
#. 3: reset_colour
msgctxt "Talk20"
msgid "<1.so>All I need is <2.se_co>1,000 rupees<3.re_co>..."
msgstr ""

msgctxt "Talk26"
msgid ""
"Once my power has been restored,\n"
"I can help you do great things, boy."
msgstr ""

#. 1: pause (short)
msgctxt "Talk27"
msgid ""
"When my power has been restored,<1.pa>\n"
"I should be able to be of use to\n"
"you, who loves horses so."
msgstr ""

#. 1: text_size
#. 2: text_size
msgctxt "Talk28"
msgid "<1.te_si>Ah-HAAAAAAAAAAA!!!<2.te_si>"
msgstr ""

