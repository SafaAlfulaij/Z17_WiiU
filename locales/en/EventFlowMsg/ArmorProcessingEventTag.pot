msgctxt "0000"
msgid "Sure."
msgstr ""

msgctxt "0001"
msgid "I'll pass."
msgstr ""

msgctxt "0005"
msgid "I'll keep my rupees."
msgstr ""

msgctxt "0014"
msgid "Here you go."
msgstr ""

#. 1: sound2
#. 2: text_size
#. 3: text_size
#. 4: pause (longer)
