msgctxt "SavedLv1_00"
msgid "<1.so>Phew...<2.pa> You really saved my skin!"
msgstr ""

#. 1: sound
#. 2: pause (long)
msgctxt "SavedLv1_01"
msgid ""
"<1.so>Oh, I'm saved!<2.pa>\n"
"Thank you!"
msgstr ""

#. 1: sound
#. 2: pause (short)
msgctxt "SavedLv2_00"
msgid ""
"<1.so>Oh, man, I barely held on to my life...<2.pa>\n"
"Thanks for your help!"
msgstr ""

#. 1: sound
#. 2: pause (short)
#. 3: pause (long)
msgctxt "SavedLv2_01"
msgid ""
"<1.so>I...<2.pa> I survived?!<3.pa>\n"
"Thank you!"
msgstr ""

#. 1: sound
#. 2: pause (long)
msgctxt "SavedLv3_00"
msgid ""
"<1.so>Phew, I was no match for those\n"
"monsters.<2.pa> Thanks for helping out!"
msgstr ""

