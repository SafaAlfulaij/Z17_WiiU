msgid "Eighth heroine?"
msgstr ""

msgctxt "0011"
msgid "Sand boots, please!"
msgstr ""

msgctxt "0012"
msgid "Sand boots?"
msgstr ""

msgctxt "0013"
msgid "The eighth heroine?"
msgstr ""

