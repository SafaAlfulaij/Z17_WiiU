msgctxt "Halt_00"
msgid ""
"<1.so><2.te_si>Whoa, whoa, whoa!<3.pa> <4.te_si>This is a <5.se_co>foot\n"
"race<6.re_co>, not a hoof race!<7.pa><8.so> <9.se_co>No horses\n"
"allowed<10.re_co>! <11.pa><12.te_si>Hey... That means I win by "
"default!<13.te_si>"
msgstr ""

#. 1: sound
#. 2: text_size
#. 3: text_size
#. 4: pause (short)
#. 5: set_colour (red)
#. 6: reset_colour
#. 7: set_colour (red)
#. 8: reset_colour
