msgctxt "talk07"
msgid ""
"<1.so>I'm catching fish!<2.pa> They'll sell in no\n"
"time flat at the general store in\n"
"<3.se_co>Zora's Domain<4.re_co>!<5.ch>"
msgstr ""

#. 1: sound
#. 2: pause (long)
#. 3: sound
msgctxt "talk08"
msgid ""
"<1.so>Yes!<2.pa> We're pretty far away right now...\n"
"\n"
"\n"
"<3.so>But there are a lot of rare fish to\n"
"catch here, so it's a lot of fun!"
msgstr ""

#. 1: sound
#. 2: pause (short)
#. 3: single_choice
msgctxt "talk09"
msgid ""
"<1.so>We got a big haul today! <2.pa>Fishing is so\n"
"much fun!<3.si_ch>"
msgstr ""

#. 1: sound
#. 2: pause (short)
#. 3: pause (long)
#. 4: sound
#. 5: text_size
#. 6: text_size
