msgid "<1.so><2.te_si>Trial three!<3.te_si>"
msgstr ""

#. 1: sound
#. 2: pause (short)
#. 3: set_colour (red)
#. 4: reset_colour
msgctxt "Npc_Kokiri008_R_Talk550"
msgid ""
"<1.so>In the eastern forest:<2.pa>\n"
"<3.se_co>The Test of Wood<4.re_co>!"
msgstr ""

#. 1: sound
#. 2: raw
#. 3: sound
msgctxt "Npc_Kokiri008_R_Talk560"
msgid ""
"<1.so>If you can overcome the Korok Trials,\n"
"you can overcome anything!\n"
"\n"
"We've also prepared some wonderful\n"
"prizes for you!<2.ra><3.so>Now show us what you're made of!"
msgstr ""

