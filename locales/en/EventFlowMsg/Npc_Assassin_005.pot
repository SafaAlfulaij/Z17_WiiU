#. 2: raw
#. 3: text_size
#. 4: text_size
#. 5: auto_advance
msgctxt "Henshin00"
msgid "<1.so><2.ra><3.te_si>I will take your life!<4.te_si><5.au_ad>"
msgstr ""

#. 1: sound
#. 2: raw
#. 3: text_size
#. 4: text_size
