#. 1: sound
msgctxt "Com_Talk_11"
msgid "<1.so>Thanks, brother!"
msgstr ""

#. 1: sound
msgctxt "Com_Talk_11b"
msgid "<1.so>Thank you, brother!"
msgstr ""

#. 1: sound
msgctxt "Com_Talk_11c"
msgid "<1.so>Thanks for your patronage, brother!"
msgstr ""

#. 1: sound
msgctxt "Com_Talk_11d"
msgid "<1.so>Thanks, little man!"
msgstr ""

