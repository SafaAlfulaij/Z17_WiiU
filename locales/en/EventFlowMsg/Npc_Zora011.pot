msgid "What about your mom?"
msgstr ""

msgctxt "0003"
msgid "Good-bye."
msgstr ""

msgctxt "0004"
msgid "Who's your dad?"
msgstr ""

msgctxt "0005"
msgid "Frogging?"
msgstr ""

msgctxt "0006"
msgid "Have some frogs."
msgstr ""

