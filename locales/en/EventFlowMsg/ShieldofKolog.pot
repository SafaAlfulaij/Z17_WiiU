msgid "Maybe later."
msgstr ""

msgctxt "0002"
msgid "Let's do this."
msgstr ""

msgctxt "0003"
msgid "Wait a second."
msgstr ""

msgctxt "0004"
msgid "I'll never give up!"
msgstr ""

